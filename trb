@PostConstruct
  private void init() {
    System.out.println(":::::::");
    final JndiTemplate jndi = new JndiTemplate();
    try {
      final DataSource ds = jndi.lookup("java:comp/env/jdbc/xxxx", DataSource.class);
      System.out.println(ds.getClass());
      System.out.println("____");
      final org.apache.tomcat.dbcp.dbcp2.BasicDataSource bds = (org.apache.tomcat.dbcp.dbcp2.BasicDataSource)ds;
      System.out.println(bds.getUrl());
      System.out.println(bds.getUsername());
    } catch (final NamingException e) {
      e.printStackTrace();
    }
  }
  
